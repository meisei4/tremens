CREATE TABLE HabitStorage (
    name TEXT PRIMARY KEY NOT NULL,
    status TEXT NOT NULL
);

insertHabit:
INSERT INTO HabitStorage(name, status)
VALUES (?, ?);

selectHabits:
SELECT * FROM HabitStorage;

updateStatus:
UPDATE HabitStorage SET status = ? WHERE name = ?;

deleteHabit:
DELETE FROM HabitStorage WHERE name = ?;
